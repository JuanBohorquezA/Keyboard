<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!--Keyboard fonts-->
    <SolidColorBrush x:Key="KeyboardTextCommon" Color="#edecf1"/>
    <SolidColorBrush x:Key="KeyboardTextCommon-Dark" Color="#222"/>
    
    <!--Keyboard Button Colors-->
    <SolidColorBrush x:Key="PrimaryButton" Color="#302f34"/>
    <SolidColorBrush x:Key="SecondaryButton" Color="#46454a"/>
    <SolidColorBrush x:Key="TertiaryButton" Color="#a4abc0"/>
    
    
    
    
    <!--Keyboard Button Triggers Pressed-->
    <Color x:Key="PrimaryButtonPressed">#1d1d20</Color>
    <Color x:Key="SecondaryButtonPressed">#828186</Color>
    <Color x:Key="TertiaryButtonPressed">#6d758e</Color>
    <!--Keyboard Button Triggers Exit-->
    <Color x:Key="PrimaryButtonExit">#302f34</Color>
    <Color x:Key="SecondaryButtonExit">#46454a</Color>
    <Color x:Key="TertiaryButtonExit">#a4abc0</Color>

</ResourceDictionary>